            <bystander allowed_species="O CO" coord_layer="square" coord_name="default" coord_offset="-1 -1 0" flag="Onn"/>
            <bystander allowed_species="O CO" coord_layer="square" coord_name="default" coord_offset="0 -2 0" flag="Onn"/>
